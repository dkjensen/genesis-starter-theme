{
	"extends": ["plugin:@wordpress/eslint-plugin/esnext"],
	"env": {
		"browser": true,
		"es6": true,
		"node": true
	},
	"globals": {
		"wp": true,
		"wpApiSettings": true,
		"wcSettings": true,
		"es6": true,
		"jQuery": true
	},
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 8,
		"ecmaFeatures": {
			"modules": true,
			"experimentalObjectRestSpread": true,
			"jsx": true
		}
	}
}
